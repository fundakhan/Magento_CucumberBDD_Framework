<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suit 1"  parallel="tests" thread-count="2">



    <listeners>
        <listener class-name="com.aventstack.extentreports.testng.listener.ExtentITestListenerAdapter"/>
    </listeners>

    <test name="Smoke Test 1">
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="Runners.Smoke_ExtentReport"></class>
        </classes>
    </test>
    <test name="Smoke Test 2">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="Runners.Smoke_ExtentReport"></class>
        </classes>
    </test>


</suite>